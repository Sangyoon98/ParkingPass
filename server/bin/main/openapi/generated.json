{
    "openapi": "3.1.1",
    "info": {
        "title": "Parking Pass API",
        "version": "1.0.0",
        "summary": "주차장 입출차 관리 API",
        "description": "번호판 인식 기반 주차장 입출차 관리 시스템",
        "license": "Apache/2.0"
    },
    "paths": {
        "/api/v1/health": {
            "get": {
                "summary": "HealthCheck",
                "requestBody": {
                    "description": "HealthCheck 요청",
                    "content": {
                        "application/json": {}
                    }
                },
                "responses": {
                    "200": {
                        "description": "HealthCheck 성공 응답",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "Health"
                ]
            }
        },
        "/api/v1/events/plate-detected": {
            "post": {
                "summary": "번호판 인식 이벤트 처리",
                "requestBody": {
                    "description": "PlateDetectedRequest 번호판 인식 요청",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/PlateDetectedRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "PlateDetectedResponse 성공 응답",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/PlateDetectedResponse"
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "ParkingEvents"
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "PlateDetectedRequest": {
                "type": "object",
                "properties": {
                    "deviceKey": {
                        "type": "string"
                    },
                    "plateNumber": {
                        "type": "string"
                    }
                }
            },
            "PlateDetectedResponse": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "action": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "ordinal": {
                                "type": "integer"
                            }
                        }
                    },
                    "plateNumber": {
                        "type": "string"
                    }
                }
            }
        }
    }
}
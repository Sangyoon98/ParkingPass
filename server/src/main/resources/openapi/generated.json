{
    "openapi": "3.1.1",
    "info": {
        "title": "Parking Pass API",
        "version": "1.0.0",
        "summary": "주차장 입출차 관리 API",
        "description": "번호판 인식 기반 주차장 입출차 관리 시스템",
        "license": "Apache/2.0"
    },
    "paths": {
        "/api/v1/health": {
            "get": {
                "summary": "HealthCheck",
                "requestBody": {
                    "description": "HealthCheck 요청",
                    "content": {
                        "application/json": {}
                    }
                },
                "responses": {
                    "200": {
                        "description": "HealthCheck 성공 응답",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "Health"
                ]
            }
        },
        "/api/v1/events/plate-detected": {
            "post": {
                "summary": "번호판 인식 이벤트 처리",
                "requestBody": {
                    "description": "PlateDetectedRequest 번호판 인식 요청",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/PlateDetectedRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "PlateDetectedResponse 성공 응답",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/PlateDetectedResponse"
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "ParkingEvents"
                ]
            }
        },
        "/api/v1/vehicles": {
            "post": {
                "summary": "등록 차량 추가",
                "requestBody": {
                    "description": "CreateVehicleRequest 등록 차량 정보",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/CreateVehicleRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "VehicleResponse 생성된 차량 정보",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "201": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/VehicleResponse"
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "Vehicles"
                ]
            },
            "get": {
                "summary": "등록 차량 목록 조회",
                "parameters": [
                    {
                        "name": "parkingLotId",
                        "in": "query",
                        "description": "주차장 ID",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "List<VehicleResponse> 차량 목록",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/VehicleResponse"
                                    }
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "Vehicles"
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "PlateDetectedRequest": {
                "type": "object",
                "properties": {
                    "deviceKey": {
                        "type": "string"
                    },
                    "plateNumber": {
                        "type": "string"
                    },
                    "capturedAt": {
                        "type": "string"
                    }
                }
            },
            "PlateDetectedResponse": {
                "type": "object",
                "properties": {
                    "action": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "ordinal": {
                                "type": "integer"
                            }
                        }
                    },
                    "sessionId": {
                        "type": "integer"
                    },
                    "plateNumber": {
                        "type": "string"
                    },
                    "isRegistered": {
                        "type": "boolean"
                    },
                    "vehicleLabel": {
                        "type": "string"
                    },
                    "vehicleCategory": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "ordinal": {
                                "type": "integer"
                            }
                        }
                    }
                }
            },
            "CreateVehicleRequest": {
                "type": "object",
                "properties": {
                    "parkingLotId": {
                        "type": "integer"
                    },
                    "plateNumber": {
                        "type": "string"
                    },
                    "label": {
                        "type": "string"
                    },
                    "category": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "ordinal": {
                                "type": "integer"
                            }
                        }
                    },
                    "memo": {
                        "type": "string"
                    }
                }
            },
            "VehicleResponse": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "parkingLotId": {
                        "type": "integer"
                    },
                    "plateNumber": {
                        "type": "string"
                    },
                    "label": {
                        "type": "string"
                    },
                    "category": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "ordinal": {
                                "type": "integer"
                            }
                        }
                    },
                    "memo": {
                        "type": "string"
                    }
                }
            }
        }
    }
}